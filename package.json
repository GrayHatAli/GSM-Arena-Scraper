{
  "name": "gsm-arena-scraper",
  "version": "2.0.0",
  "description": "Independent GSM Arena scraper with RESTful API for mobile phone data",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js server",
    "scrape": "node src/index.js cli",
    "test": "node src/index.js test",
    "dev": "nodemon src/index.js server",
    "cli": "node src/index.js cli",
    "install-deps": "npm install",
    "build": "echo 'No build step required'",
    "docker:build": "docker build -t gsm-arena-scraper .",
    "docker:run": "docker run -p 3002:3002 gsm-arena-scraper"
  },
  "keywords": [
    "scraper",
    "gsm-arena",
    "mobile-phones",
    "puppeteer",
    "web-scraping",
    "api",
    "rest",
    "independent"
  ],
  "author": "GrayHatAli@gmail.com",
  "license": "MIT",
  "engines": {
    "node": "22.x"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "puppeteer": "^24.15.0",
    "swagger-ui-express": "^5.0.1",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:GrayHatAli/GSM-Arena-Scraper.git"
  },
  "homepage": "https://github.com/GrayHatAli/GSM-Arena-Scraper#readme",
  "bugs": {
    "url": "https://github.com/GrayHatAli/GSM-Arena-Scraper/issues"
  }
}
