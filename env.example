# GSM Arena Scraper Configuration

# Server Configuration
PORT=3002
HOST=0.0.0.0
NODE_ENV=production

# Scraping Configuration
MIN_YEAR=2022
MODELS_PER_BRAND=10
REQUEST_DELAY=1000
MODEL_DELAY=2000
BRAND_DELAY=3000

# Target Brands (comma-separated)
TARGET_BRANDS=apple,samsung,xiaomi,google,huawei,oppo,nothing,realme,honor

# Output Configuration
OUTPUT_FILE=./data/scraped-data.json
LOG_LEVEL=info

# Puppeteer Configuration
PUPPETEER_HEADLESS=true
PUPPETEER_TIMEOUT=30000

# API Configuration
API_TIMEOUT=300000
CORS_ORIGIN=*
REDIS_URL=redis://localhost:6379
